{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "bootstrap": "4.0.0-alpha.5",
    "node-sass": "^3.13.0",
    "react-scripts": "0.7.0"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "isomorphic-fetch": "2.2.1",
    "material-ui": "^0.16.4",
    "mdi": "^1.7.22",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-router": "3.0.0",
    "react-slick": "^0.14.5",
    "react-tap-event-plugin": "2.0.1",
    "slick-carousel": "^1.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "REACT_APP_API_URL=http://localhost:8080 npm start",
