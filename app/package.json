{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "bootstrap": "4.0.0-alpha.5",
    "node-sass": "^3.11.0",
    "react-scripts": "0.7.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.16.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^1.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "REACT_APP_API_URL=http://localhost:8080 npm start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "create:grid": "node-sass src/app/Grid.scss src/app/Grid.css --output-style expanded --indent-type space --indent-width 4",
    "create:grid:watch": "npm run create:grid -- --watch",
    "release": "npm run build && ./scripts/deploy.sh"
  }
